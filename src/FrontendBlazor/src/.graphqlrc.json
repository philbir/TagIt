{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
        "name": "TagItClient",
        "url": "https://tagit.birbaum.me/graphql",
        "dependencyInjection": true,
        "strictSchemaValidation": true,
        "hashAlgorithm": "md5",
        "useSingleFile": true,
        "requestStrategy": "Default",
        "outputDirectoryName": "Generated",
        "noStore": false,
        "emitGeneratedCode": true,
        "razorComponents": true,
        "records": {
            "inputs": false,
            "entities": false
        },
        "transportProfiles": [
            {
                "default": "Http",
                "subscription": "WebSocket"
            }
        ]
    }
  }
}
